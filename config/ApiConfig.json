{
    "Endpoints": {
        "Local": "https://prov.beameio.net",
        "Online": "https://prov.beameio.net",
        "AuthServer": "https://registration.beameio.net",
        "CertEndpoint": "https://beameio-net-certs.s3.amazonaws.com"
    },
    "RetryAttempts" : 10,
    "Actions": {
        "DeveloperApi": {
            "CompleteRegistration": {
                "endpoint": "/api/v1/auth/dev/complete"
            },
            "RestoreCert": {
                "endpoint": "/api/v1/auth/dev/cert/restore"
            },
            "CreateDeveloper": {
                "endpoint": "/api/v1/dev/create"
            },
            "GetCert": {
                "endpoint": "/api/v1/dev/cert/get"
            },
            "RenewCert": {
                "endpoint": "/api/v1/dev/cert/renew"
            },
            "RevokeCert": {
                "endpoint": "/api/v1/dev/cert/revoke"
            },
            "UpdateProfile": {
                "endpoint": "/api/v1/dev/profile/update"
            },
            "GetStats": {
                "endpoint": "/api/v1/dev/stats"
            }
        },
        "AtomApi": {
            "CreateAtom": {
                "endpoint": "/api/v1/atom/save"
            },
            "DeleteAtom": {
                "endpoint": "/api/v1/atom/delete"
            },
            "UpdateAtom": {
                "endpoint": "/api/v1/atom/update"
            },
            "GetCert": {
                "endpoint": "/api/v1/atom/cert/get"
            },
            "RenewCert": {
                "endpoint": "/api/v1/atom/cert/renew"
            },
            "RevokeCert": {
                "endpoint": "/api/v1/atom/cert/revoke"
            },
            "GetStats": {
                "endpoint": "/api/v1/atom/stats"
            }
        },
        "EdgeClient":{
            "CreateEdgeClient": {
                "endpoint" : "/api/v1/client/register"
            },
            "DeleteEdgeClient": {
                "endpoint" : "/api/v1/client/delete"
            },
            "GetCert": {
                "endpoint" : "/api/v1/client/cert/get"
            },
            "RenewCert": {
                "endpoint": "/api/v1/client/cert/renew"
            },
            "RevokeCert": {
                "endpoint": "/api/v1/client/cert/revoke"
            },
            "GetStats": {
                "endpoint": "/api/v1/client/stats"
            }
        }
    }
}
